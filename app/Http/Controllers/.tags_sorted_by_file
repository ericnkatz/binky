HomeController	HomeController.php	/^class HomeController extends Controller {$/;"	c
index	HomeController.php	/^	public function index()$/;"	f
ProductsController	ProductsController.php	/^class ProductsController extends Controller {$/;"	c
amazon	ProductsController.php	/^				$amazon = new Amazon;$/;"	v
create	ProductsController.php	/^	public function create()$/;"	f
destroy	ProductsController.php	/^	public function destroy($id)$/;"	f
edit	ProductsController.php	/^	public function edit($id)$/;"	f
index	ProductsController.php	/^	public function index()$/;"	f
products	ProductsController.php	/^		$products = Product::paginate( $limit = 30 );$/;"	v
result	ProductsController.php	/^				$result = $search = $amazon->search($product->name);$/;"	v
result	ProductsController.php	/^				\/\/ $result = Cache::remember('amz1n_'.md5($product->name), 1440, function() use ($amazon, $product) {$/;"	v
search	ProductsController.php	/^				\/\/ 		$search = $amazon->search($product->name);$/;"	v
show	ProductsController.php	/^	public function show($id)$/;"	f
store	ProductsController.php	/^	public function store()$/;"	f
update	ProductsController.php	/^	public function update($id)$/;"	f
